{
  "build": {
    "beforeBuildCommand": "npm run build",
    "beforeDevCommand": "npm run build",
    "devPath": "../public",
    "distDir": "../public"
  },
  "package": {
    "productName": "identia",
    "version": "0.4.0"
  },
  "tauri": {
    "allowlist": {
      "all": true
    },
    "bundle": {
      "active": true,
      "category": "P2P",
      "copyright": "iohzrd",
      "deb": {
        "depends": [],
        "useBootstrapper": false
      },
      "externalBin": ["../node_modules/go-ipfs/go-ipfs/ipfs"],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "identia",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "minimumSystemVersion": "",
        "providerShortName": null,
        "signingIdentity": null,
        "useBootstrapper": false
      },
      "resources": [],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": ""
    },
    "systemTray": {
      "iconPath": "icons/icon.png"
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 1080,
        "resizable": true,
        "title": "identia",
        "width": 1920
      }
    ]
  }
}
