{
  "name": "identia",
  "version": "0.1.0",
  "description": "Decentralized, censorship resistant social media on IPFS",
  "productName": "identia",
  "author": "iohzrd <iohzrd@gmail.com>",
  "license": "GPL-3.0",
  "private": true,
  "scripts": {
    "build": "rollup -c",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "dev": "rollup -c -w",
    "info": "tauri info",
    "postinstall": "npm run renameIpfsBinary",
    "renameIpfsBinary": "node scripts/renameIpfsBinary.js",
    "serve": "sirv public --no-clear",
    "start": "tauri dev",
    "tauri": "tauri",
    "tauri:build": "tauri build",
    "tauri:build-debug": "tauri build -- --debug",
    "tauri:dev": "tauri dev",
    "update": "ncu -u && npm install && cd src-tauri && cargo update"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-typescript": "^8.3.0",
    "@splidejs/svelte-splide": "^0.1.13",
    "@tauri-apps/cli": "^1.0.0-beta.10",
    "@tsconfig/svelte": "^2.0.1",
    "buffer": "^6.0.3",
    "carbon-components-svelte": "^0.50.1",
    "carbon-icons-svelte": "^10.38.0",
    "carbon-preprocess-svelte": "^0.6.0",
    "exifremove": "^1.0.1",
    "go-ipfs": "^0.10.0",
    "hotkeys-js": "^3.8.7",
    "ipfs-http-client": "^54.0.2",
    "linkify-html": "^3.0.4",
    "linkify-string": "^3.0.4",
    "linkifyjs": "^3.0.4",
    "npm-check-updates": "^12.0.2",
    "rollup": "^2.60.1",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "string-strip-html": "^9.1.0",
    "svelte": "^3.44.2",
    "svelte-check": "^2.2.10",
    "svelte-inview": "^2.1.1",
    "svelte-preprocess": "^4.9.8",
    "svelte-spa-router": "^3.2.0",
    "timeago.js": "^4.0.2",
    "tslib": "^2.3.1",
    "typescript": "^4.5.2",
    "webcam-easy": "^1.0.5"
  },
  "dependencies": {
    "@tauri-apps/api": "^1.0.0-beta.8",
    "sirv-cli": "^1.0.14"
  }
