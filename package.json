{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "rollup -c",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "dev": "rollup -c -w",
    "info": "tauri info",
    "postinstall": "npm run renameIpfsBinary",
    "renameIpfsBinary": "node scripts/renameIpfsBinary.js",
    "start": "sirv public --no-clear",
    "tauri": "tauri",
    "tauri:build": "tauri build",
    "tauri:build-debug": "tauri build -- --debug",
    "tauri:dev": "tauri dev",
    "update": "ncu -u && npm install && cd src-tauri && cargo update"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-typescript": "^8.2.5",
    "@tauri-apps/cli": "^1.0.0-beta.9",
    "@tsconfig/svelte": "^2.0.1",
    "buffer": "^6.0.3",
    "carbon-components-svelte": "^0.42.2",
    "carbon-icons-svelte": "^10.36.0",
    "carbon-preprocess-svelte": "^0.6.0",
    "exifremove": "^1.0.1",
    "file-type": "^16.5.3",
    "go-ipfs": "^0.9.1",
    "hotkeys-js": "^3.8.7",
    "npm-check-updates": "^11.8.3",
    "rollup": "^2.56.2",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.42.2",
    "svelte-check": "^2.2.4",
    "svelte-preprocess": "^4.7.4",
    "timeago.js": "^4.0.2",
    "tslib": "^2.3.1",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@tauri-apps/api": "^1.0.0-beta.6",
    "sirv-cli": "^1.0.14"
  }
}
