{
  "name": "identia",
  "version": "0.4.0",
  "description": "Decentralized, censorship resistant social media on IPFS",
  "productName": "identia",
  "author": "iohzrd <iohzrd@gmail.com>",
  "license": "AGPL-3.0",
  "private": true,
  "scripts": {
    "build": "rollup -c",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "dev": "rollup -c -w",
    "info": "tauri info",
    "postinstall": "npm run renameIpfsBinary",
    "renameIpfsBinary": "node scripts/renameIpfsBinary.js",
    "serve": "sirv public --no-clear",
    "start": "tauri dev",
    "tauri": "tauri",
    "tauri:build": "tauri build",
    "tauri:build-debug": "tauri build -- --debug",
    "tauri:dev": "tauri dev",
    "update": "ncu -u && npm install && npx tauri deps update && cd src-tauri && cargo update"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-replace": "^3.0.1",
    "@rollup/plugin-typescript": "^8.3.0",
    "@splidejs/svelte-splide": "^0.1.17",
    "@tauri-apps/cli": "^1.0.0-beta.10",
    "@tsconfig/svelte": "^3.0.0",
    "buffer": "^6.0.3",
    "carbon-components-svelte": "^0.54.0",
    "carbon-icons-svelte": "^10.44.3",
    "carbon-preprocess-svelte": "^0.6.0",
    "exifremove": "^1.0.1",
    "go-ipfs": "^0.11.0",
    "hotkeys-js": "^3.8.7",
    "ipfs-http-client": "^55.0.0",
    "is-ipfs": "^6.0.2",
    "linkify-html": "^3.0.5",
    "linkify-string": "^3.0.4",
    "linkifyjs": "^3.0.5",
    "npm-check-updates": "^12.1.0",
    "rollup": "^2.64.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "string-strip-html": "^9.1.5",
    "svelte": "^3.46.2",
    "svelte-check": "^2.3.0",
    "svelte-inview": "^2.1.1",
    "svelte-pdf": "^1.0.10",
    "svelte-preprocess": "^4.10.2",
    "svelte-spa-router": "^3.2.0",
    "timeago.js": "^4.0.2",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4",
    "webcam-easy": "^1.0.5"
  },
  "dependencies": {
    "@tauri-apps/api": "^1.0.0-beta.8",
    "sirv-cli": "^2.0.2"
  }
